parameters:
	tmpDir: build/phpstan
	level: 5
	paths:
		- app/
		- tests/
	bootstrapFiles:
		- vendor/codeigniter4/framework/system/Test/bootstrap.php
	excludePaths:
		- app/Cells/category_list_cell.php
		- app/Cells/mute_thread.php
		- app/Config/*
		- app/Views/*
		- app/Libraries/TextFormatter.php
	ignoreErrors:
	    - '#Call to deprecated method getStatusCode\(\) of class CodeIgniter\\HTTP\\Response:#'
	    - '#Call to an undefined method CodeIgniter\\Database\\ConnectionInterface::[A-Za-z].+\(\)#'
	    - '#Cannot access property [\$a-z_]+ on (array|object)#'
	    - '#Cannot call method [fill\|\|link\|\|hasChanged].+\(\) on array\|object.#'
	    - '#Call to an undefined method CodeIgniter\\HTTP\\RedirectResponse::[hxLocation\|\|hxRedirect\|\|hxRefresh].+\(\)#'
	    - '#Property CodeIgniter\\Controller::\$validator \(CodeIgniter\\Validation\\ValidationInterface\) on left side of \?\? is not nullable.#'
	    -
	    	message: '#Call to an undefined method CodeIgniter\\Shield\\Auth::forget\(\).#'
	    	paths:
	    	    - app/Controllers/Account/SecurityController.php
	    -
	    	message: '#Call to deprecated function random_string\(\):#'
	    	paths:
	    	    - app/Models/Factories/UserFactory.php
	    	    - tests/Controllers/DiscussionControllerTest.php
	    -
	    	message: '#Call to an undefined method App\\Models\\UserModel::transException\(\).#'
	    	paths:
	    	    - app/Controllers/Account/SecurityController.php
	    -
	    	message: '#Call to an undefined method App\\Models\\UserModel::transComplete\(\).#'
	    	paths:
	    	    - app/Controllers/Account/SecurityController.php
	treatPhpDocTypesAsCertain: false
	universalObjectCratesClasses:
		- CodeIgniter\Entity
		- CodeIgniter\Entity\Entity
		- Faker\Generator
	scanDirectories:
		- vendor/codeigniter4/framework/system/Helpers
		- vendor/codeigniter4/settings/src/Helpers
	dynamicConstantNames:
		- APP_NAMESPACE
		- CI_DEBUG
		- ENVIRONMENT
